{
    "name": "sof-language-support",
    "displayName": "SOF language support",
    "description": "Language support for the experimental Stack with Objects and Functions programming language.",
    "license": "MIT",
    "version": "0.0.4",
    "author": {
        "name": "kleinesfilmr√∂llchen"
    },
    "publisher": "kleinesfilmroellchen",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/kleinesfilmroellchen/sof-syntaxhighlight",
        "type": "git"
    },
    "main": "./out/sof-langfeatures.js",
    "activationEvents": [
        "onLanguage:klfr-sof"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.40.0",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "eslint": "^6.8.0",
        "@typescript-eslint/parser": "^2.30.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "glob": "^7.1.6",
        "mocha": "^10.2.0",
        "typescript": "^3.8.3"
    },
    "contributes": {
        "languages": [
            {
                "id": "klfr-sof",
                "aliases": [
                    "Stack with Objects and Functions (SOF)",
                    "klfr-sof",
                    "SOF"
                ],
                "extensions": [
                    ".sof",
                    ".stackof"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "klfr-sof",
                "scopeName": "source.klfr-sof",
                "path": "./syntaxes/klfr-sof.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "helloworld.helloWorld",
                "title": "Hello World"
            }
        ]
    }
}
