{
    "name": "sof-language-support",
    "displayName": "sof-language-support",
    "description": "Language support for the experimental SOF programming language",
    "version": "0.0.1",
    "author": {
        "name": "kleinesfilmr√∂llchen"
    },
    "publisher": "kleinesfilmroellchen",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "url": "https://github.com/kleinesfilmroellchen/sof-syntaxhighlight",
        "type": "git"
    },
    "contributes": {
        "languages": [
            {
                "id": "klfr-sof",
                "aliases": [
                    "Stack with Objects and Functions (SOF)",
                    "klfr-sof",
                    "SOF"
                ],
                "extensions": [
                    ".sof",
                    ".stackof"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "klfr-sof",
                "scopeName": "source.klfr-sof",
                "path": "./syntaxes/klfr-sof.tmLanguage.json"
            }
        ]
    }
}